#include "CentralNetworkConfig.h"

namespace inet{

class CentralNetworkConfig : public cSimpleModule
{
    public:
        int registerStream(Demand d);

    protected:
        virtual void initialize() override;
};

Define_Module(CentralNetworkConfig);

void CentralNetworkConfig::initialize()
{
    EV << "Hello World!\n";
}

int CentralNetworkConfig::registerStream(Demand d)
{
    cout<< this->getFullName() << " registerStream has been called for Demand "<< d.index << endl;
}

}
